        "react-test-renderer": "~16.9.0",
        "rimraf": "^2.6.2"
    },
    "peerDependencies": {
        "@material-ui/core": "^4.4.0",
        "@material-ui/icons": "^4.4.0",
        "react": "^16.9.0",
        "react-dom": "^16.9.0"
    },
    "dependencies": {
        "autosuggest-highlight": "^3.1.1",
        "classnames": "~2.2.5",
